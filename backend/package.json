{
  "engines" : {
    "node" : ">14.15.0"
  },
  "scripts": {
    "start": "keystone-next start",
    "build": "keystone-next build",
    "dev": "keystone-next",
    "seed-data": "keystone-next --seed-data"
  },
  "eslintConfig": {
    "extends": "wesbos/typescript.js",
    "rules": {
      "@typescript-eslint/no-unsafe-assignment": 0,
      "import/no-unresolved": 0,
      "@typescript-eslint/no-unsafe-return": 0,
      "@typescript-eslint/no-unsafe-member-access": 0
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 14,
            "browsers": [
              "last 2 chrome versions",
              "last 2 firefox versions",
              "last 2 safari versions",
              "last 2 edge versions"
            ]
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@keystone-next/admin-ui": "12.0.1",
    "@keystone-next/auth": "18.0.0",
    "@keystone-next/cloudinary": "3.0.4",
    "@keystone-next/fields": "5.3.0",
    "@keystone-next/keystone": "13.0.0",
    "@keystone-next/types": "15.0.0",
    "@keystonejs/server-side-graphql-client": "2.1.0",
    "@types/nodemailer": "6.4.1",
    "dotenv": "8.2.0",
    "next": "10.0.9",
    "nodemailer": "6.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "stripe": "8.139.0"
  },
  "devDependencies": {
    "@types/node": "14.14.35",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.22.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-wesbos": "2.0.0-beta.4",
    "eslint-plugin-html": "6.1.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "2.2.1",
    "typescript": "4.2.3"
  }
}
