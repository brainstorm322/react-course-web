{
  "scripts": {
    "predev": "npm run codegen",
    "dev": "next -p 7777",
    "build": "next build",
    "prestart": "npm run codegen",
    "start": "next start",
    "test": "NODE_ENV=test jest --watch",
    "heroku-postbuild": "next build",
    "preinstall": "npx npm-force-resolutions",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "3.3.11",
    "@apollo/link-error": "2.0.0-beta.3",
    "@apollo/react-ssr": "4.0.0",
    "@stripe/react-stripe-js": "1.2.2",
    "@stripe/stripe-js": "1.13.0",
    "apollo-upload-client": "14.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-styled-components": "1.12.0",
    "date-fns": "2.17.0",
    "dotenv": "8.2.0",
    "downshift": "6.1.0",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "graphql-upload": "11.0.0",
    "lodash.debounce": "4.0.8",
    "next": "10.0.7",
    "next-with-apollo": "5.1.1",
    "nprogress": "0.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-transition-group": "4.4.1",
    "styled-components": "5.2.1",
    "waait": "1.0.5"
  },
  "devDependencies": {
    "@apollo/react-testing": "4.0.0",
    "@babel/core": "7.13.1",
    "@babel/preset-env": "7.13.0",
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "1.1.2",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.7.2",
    "@types/dotenv": "8.2.0",
    "@types/lodash.debounce": "4.0.6",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/react-transition-group": "4.4.1",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "casual": "1.6.2",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.0.0",
    "eslint-config-wesbos": "2.0.0-beta.4",
    "eslint-plugin-html": "6.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1",
    "typescript": "4.1.5"
  },
  "eslintConfig": {
    "extends": "wesbos/typescript.js",
    "rules": {
      "@typescript-eslint/ban-ts-comment": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/indent": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-unsafe-assignment": 0,
      "@typescript-eslint/no-unsafe-call": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-return": 0,
      "@typescript-eslint/restrict-plus-operands": 0,
      "@typescript-eslint/restrict-template-expressions": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "jsx-a11y/img-redundant-alt": 0,
      "no-plusplus": 0,
      "no-restricted-globals": 0,
      "no-use-before-define": 0,
      "no-void": 0,
      "prefer-destructuring": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-props-no-spreading": 0
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
